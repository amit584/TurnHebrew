{
  "manifest_version": 3,
  "version": "1.1.0",
  "name": "turnHebrew",
  "description": "turns text from english to hebrew",
  "background": {
    "service_worker": "eventPage-wrapper.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "notifications",
    "contextMenus",
    "clipboardWrite"
  ]
}
